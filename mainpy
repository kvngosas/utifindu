import kivy
import webbrowser
from kivy.app import App
from kivy.factory import Factory
from kivy.core.window import Window
from kivymd.app import MDApp
from kivy.uix.gridlayout import GridLayout
from kivy.uix.boxlayout import BoxLayout
from kivy.uix.floatlayout import FloatLayout
from kivy.uix.label import Label
from kivy.uix.filechooser import FileChooser
from kivy.uix.image import Image
from kivy.uix.textinput import TextInput
from kivy.uix.button import Button
from kivy.animation import Animation
from kivy.uix.popup import Popup


class UtiFindApp(App):
    def build(self):
        self.icon = "favicon.png"
        self.window = GridLayout()
        self.window.cols = 1
        self.window.size_hint = (0.7, 0.8)
        self.window.pos_hint = {"center_x": 0.5, "center_y": 0.5}

        self.image = Image(source="logo.png")
        self.window.add_widget(self.image)

        self.welcome = Label(
            text="The First Utility Finder App",
            font_size=20,
            color='#b6fcff',
            size_hint=(1, 0.3),
        )
        self.window.add_widget(self.welcome)

        self.contract = TextInput(
            multiline=False,
            padding_y=(18, 18),
            size_hint=(1, 0.2),
            hint_text='Paste contract address/name here'
        )
        Window.softinput_mode = 'below_target'
        Window.softinput_mode = 'pan'
        self.window.add_widget(self.contract)

        self.space = Label(
            text="",
            size_hint=(0.05, 0.05),
        )
        self.window.add_widget(self.space)

        self.button = Button(
            text='UFind',
            size_hint=(1, 0.2),
            bold=True,
            background_color='b6fcff'
        )
        self.button.bind(on_press=self.callback)
        self.window.add_widget(self.button)

        return self.window


